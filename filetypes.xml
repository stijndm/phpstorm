    <extensionMap>
      <mapping ext="info" type="Ini" />
      <mapping ext="profile" type="PHP" />
      <mapping ext="test" type="PHP" />
      <mapping ext="install" type="PHP" />
      <mapping ext="theme" type="PHP" />
      <mapping pattern="LICENSE" type="PLAIN_TEXT" />
      <mapping pattern=".env" type="PLAIN_TEXT" />
      <mapping ext="xliff" type="PLAIN_TEXT" />
      <mapping pattern="no_active_campaign" type="Twig" />
    </extensionMap>
  </component>
</application>